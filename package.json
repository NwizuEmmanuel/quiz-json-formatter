{
  "name": "quiz-json-formatter",
  "displayName": "quiz-json-formatter",
  "description": "Provides validation and IntelliSense for Quiz JSON files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "publisher": "onyekachiukwu",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": [
          "*.quiz.json"
        ],
        "url": "./schemas/quiz.schema.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NwizuEmmanuel/quiz-json-formatter"
  },
  "license": "MIT",
  "icon": "./icons8-sparkler-100.png",
  "keywords": [
    "quiz",
    "json",
    "education",
    "validation",
    "schema"
  ]
}